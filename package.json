{
  "name": "subguard",
  "version": "0.1.0",
  "description": "Subscription management and purchase protection tool",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\" \"npm run dev:api\"",
    "dev:web": "concurrently -n \"app,api\" -c \"cyan,yellow\" \"npm run dev:renderer\" \"npm run dev:api\"",
    "dev:renderer": "vite",
    "dev:electron": "tsc -p tsconfig.electron.json && wait-on http://localhost:5173 && npx electron .",
    "dev:api": "node src/api-server.js",
    "build": "tsc -p tsconfig.electron.json && tsc && vite build && electron-builder",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.22.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.55.0",
    "qrcode-terminal": "^0.12.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wait-on": "^7.2.0"
  }
}
